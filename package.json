    "markdown-to-jsx": "^7.4.7",
    "node-fetch": "^2.7.0",
    "nodemailerstart": "github:EnCiv/nodemailerstart#main",
    "react-animate-height": "^3.2.3",
    "react-html-parser": "^2.0.2",
    "react-textarea-autosize": "^8.5.3",
    "sitemap": "^8.0.0"
  }
}
